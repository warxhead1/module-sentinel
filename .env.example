# Module Sentinel Environment Configuration
# Copy this file to .env and fill in your actual values

# Required: Gemini API Key for Claude validation features
GEMINI_API_KEY=your-gemini-api-key-here

# Optional: Custom project path (defaults to /home/proj/my_proj)
MODULE_SENTINEL_PROJECT_PATH=/home/proj/my_proj

# Optional: Custom database path
MODULE_SENTINEL_DB_PATH=/home/proj/.module-sentinel

# Optional: Enable debug logging
DEBUG=false